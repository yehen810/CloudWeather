<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CloudsWeather" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->

        <item name="colorSecondary">@color/colorDarkAccent</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>

        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

    </style>
    
    <style name="searchFrameLayout" parent="Theme.CloudsWeather">
        <item name="android:background">@color/purple_700</item>
    </style>


</resources>